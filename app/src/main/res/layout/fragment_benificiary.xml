                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:background="#fff"
                            android:clickable="true"
                            android:elevation="4dp"
                            android:padding="3dp"
                            android:text="SI No. of The Proceeding :" />

                        <TextView
                            android:id="@+id/basicBenificiaryDateOfGrounding"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:background="#fff"
                            android:clickable="true"
                            android:elevation="4dp"
                            android:padding="3dp"
                            android:text="Date of Grounding :" />

                        <TextView
                            android:id="@+id/basicBenificiaryplaceOfGrounding"
