                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:background="#fff"
                        android:clickable="true"
                        android:elevation="4dp"
                        android:padding="5dp"
                        android:text="Father/Husband Name : \n" />
                    <TextView
                        android:id="@+id/basicBenificiaryVillage"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:background="#fff"
                        android:clickable="true"
                        android:elevation="4dp"
                        android:padding="5dp"
                        android:text="Village : " />
                    <TextView
                        android:id="@+id/basicBenificiaryMandal"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:background="#fff"
                        android:clickable="true"
                        android:elevation="4dp"
                        android:padding="5dp"
                        android:text="Mandal :" />
                    <TextView
                        android:id="@+id/basicBenificiaryDistrict"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:background="#fff"
                        android:clickable="true"
                        android:elevation="4dp"
                        android:padding="5dp"
                        android:text="District : " />
