                        android:layout_margin="5dp"
                        android:background="#fff"
                        android:clickable="true"
                        android:elevation="4dp"
                        android:padding="3dp"
                        android:text="Department :" />
                    <TextView
                        android:id="@+id/basicBenificiaryScheme"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:background="#fff"
                        android:clickable="true"
                        android:elevation="4dp"
                        android:padding="3dp"
                        android:text="Scheme :" />

                    <TextView
                        android:id="@+id/basicBenificiaryUnitCost"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:background="#fff"
                        android:clickable="true"
                        android:elevation="4dp"
                        android:padding="3dp"
                        android:text="Unit Cost :" />

                    <TextView
                        android:id="@+id/basicBenificiarySubsidy"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:background="#fff"
                        android:clickable="true"
                        android:elevation="4dp"
                        android:padding="3dp"
                        android:text="Subsidy :" />
                    <TextView
                        android:id="@+id/basicBenificiaryBenificiaryContrib"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:background="#fff"
                        android:clickable="true"
                        android:elevation="4dp"
                        android:padding="3dp"
                        android:text="Benificiary Contrib :" />
